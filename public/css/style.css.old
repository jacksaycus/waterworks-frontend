.modal-close, .replace-close, .replace-over-close {
  position: absolute;
  right: 0;
  top: 0;
  width: 60px;
  height: 60px;
  cursor: pointer;
  z-index: 999;
}
.modal-close:after, .modal-close:before, .replace-close:before, .replace-close:after, .replace-over-close:before, .replace-over-close:after {
  content: '';
  position: absolute;
  width: 2px;
  height: 35px;
  background: #222;
  display: block;
  transform: rotate(45deg);
  left: 50%;
  margin: 9px 0 0 0;
  top: 3px;
}
.modal-close:before, .replace-close:before, .replace-over-close:before, .replace-over-close:before {
