/* common */
* {
  box-sizing: border-box;
}
html, body {
  height: 100%;
  -webkit-tap-highlight-color: transparent;
}
body {
  font-family: 'Roboto', 'Noto Sans KR', Arial, sans-serif;
  font-weight: normal;
  font-size: 16px;
  line-height: 1.5;
  color: #222;
  background: #fff;
  letter-spacing: -0.05em;
  word-break: keep-all;
  /* min-width: 768px;*/
  width: 100%;
}
button, input, textarea, table {
  font-family: 'Roboto', 'Noto Sans KR', Arial, sans-serif;
  line-height: 1.4;
  letter-spacing: -0.05em;
}
/*input*/
input, textarea {
  width: 100%;
  background-color: transparent;
  border: 1px solid #ddd;
  padding: 8px 10px 8px 10px;
  font-size: 15px;
  color: #222;
  border-radius: 3px;
  height: 35px;
  background: #fff;
}
input:focus, textarea:focus {
  border-color: #222;
  transition: all .3s;
}
textarea {
  font-size: 14px;
  overflow: hidden;
  padding: 16px;
  display: block;
}
